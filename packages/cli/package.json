{
  "name": "@lherron/agent-spaces",
  "version": "0.2.1",
  "description": "Command line interface for Agent Spaces v2",
  "repository": {
    "type": "git",
    "url": "https://github.com/lherron/agent-spaces-v2"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "asp": "./bin/asp.js"
  },
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./core.d.ts",
      "import": "./core.js"
    },
    "./resolver": {
      "types": "./resolver.d.ts",
      "import": "./resolver.js"
    },
    "./store": {
      "types": "./store.d.ts",
      "import": "./store.js"
    },
    "./materializer": {
      "types": "./materializer.d.ts",
      "import": "./materializer.js"
    },
    "./git": {
      "types": "./git.d.ts",
      "import": "./git.js"
    },
    "./engine": {
      "types": "./engine.d.ts",
      "import": "./engine.js"
    },
    "./claude": {
      "types": "./claude.d.ts",
      "import": "./claude.js"
    },
    "./lint": {
      "types": "./lint.d.ts",
      "import": "./lint.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "node_modules/spaces-*",
    "core.js",
    "core.d.ts",
    "resolver.js",
    "resolver.d.ts",
    "store.js",
    "store.d.ts",
    "materializer.js",
    "materializer.d.ts",
    "git.js",
    "git.d.ts",
    "engine.js",
    "engine.d.ts",
    "claude.js",
    "claude.d.ts",
    "lint.js",
    "lint.d.ts"
  ],
  "bundledDependencies": [
    "agent-spaces",
    "spaces-config",
    "spaces-execution"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "prepack": "mkdir -p node_modules && for pkg in config runtime harness-claude harness-pi harness-pi-sdk harness-codex execution; do mkdir -p node_modules/spaces-\"$pkg\" && cp -r ../\"$pkg\"/dist node_modules/spaces-\"$pkg\"/ && cp ../\"$pkg\"/package.json node_modules/spaces-\"$pkg\"/; done && mkdir -p node_modules/agent-spaces && cp -r ../agent-spaces/dist node_modules/agent-spaces/ && cp ../agent-spaces/package.json node_modules/agent-spaces/"
  },
  "dependencies": {
    "agent-spaces": "*",
    "spaces-config": "*",
    "spaces-execution": "*",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "figures": "^6.1.0",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.8.3"
  }
}
