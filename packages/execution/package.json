{
  "name": "spaces-execution",
  "version": "0.1.0",
  "private": true,
  "description": "Run-time volatility: harness execution, sessions, Claude/Pi invocation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./agent-sdk": {
      "bun": "./src/agent-sdk/index.ts",
      "types": "./dist/agent-sdk/index.d.ts",
      "import": "./dist/agent-sdk/index.js"
    },
    "./session": {
      "bun": "./src/session/index.ts",
      "types": "./dist/session/index.d.ts",
      "import": "./dist/session/index.js"
    },
    "./pi-session": {
      "bun": "./src/pi-session/index.ts",
      "types": "./dist/pi-session/index.d.ts",
      "import": "./dist/pi-session/index.js"
    },
    "./claude": {
      "bun": "./src/claude/index.ts",
      "types": "./dist/claude/index.d.ts",
      "import": "./dist/claude/index.js"
    },
    "./harness": {
      "bun": "./src/harness/index.ts",
      "types": "./dist/harness/index.d.ts",
      "import": "./dist/harness/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "spaces-config": "*",
    "spaces-runtime": "*",
    "spaces-harness-claude": "*",
    "spaces-harness-pi": "*",
    "spaces-harness-pi-sdk": "*",
    "spaces-harness-codex": "*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.8.3"
  }
}
